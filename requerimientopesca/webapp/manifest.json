{
    "_version": "1.32.0",
    "sap.app": {
        "id": "com.tasa.requerimientopesca",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "ach": "ach",
        "crossNavigation": {
                    "inbounds": {
                        "tasa-com-requerimientopesca-inbound": {
                            "signature": {
                                "parameters": {},
                                "additionalParameters": "allowed"
                            },
                            "semanticObject": "RequerimientoPesca",
                            "action": "display",
                            "title": "{{flpTitle}}",
                            "subTitle": "{{flpSubtitle}}",
                            "icon": "sap-icon://legend"
                        }
                    }
                }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "rootView": {
            "viewName": "com.tasa.requerimientopesca.view.Main",
            "type": "XML",
            "async": true,
            "id": "Main"
        },
        "dependencies": {
            "minUI5Version": "1.93.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.ui.layout": {}
            }
        },
		"config": {
			"sample": {
				"files": [
                    "DlgCentro.fragment.xml",
                    "DlgNewReg.fragment.xml",
                    "Main.view.xml",
                    "Main.controller.js",
					"manifest.json"
				]
			}
		},
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.tasa.requerimientopesca.i18n.i18n"
                }
            },
            "modelReqPesca": {
                "preload": true,
                "type": "sap.ui.model.json.JSONModel"
            }            
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "com.tasa.requerimientopesca.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "RouteMain",
                    "pattern": "RouteMain",
                    "target": [
                        "TargetMain"
                    ]
                }
            ],
            "targets": {
                "TargetMain": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "Main",
                    "viewName": "Main"
                }
            }
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "ReqPescaDistFlota"
    }
}